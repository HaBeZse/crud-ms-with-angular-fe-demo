{
  "name": "crud-ms-with-angular-fe-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "private": "true",
  "workspaces": [
    "services/profile-service",
    "services/address-service",
    "frontend"
  ],
  "scripts": {
    "lint": "npm -ws run lint",
    "format": "npm -ws run format",
    "test": "npm -ws run test",
    "test:cov": "npm -ws run test:cov",
    "build": "npm -ws run build",
    "infra:up": "docker compose --project-directory infra -f infra/docker-compose.yml up -d",
    "infra:down": "docker compose --project-directory infra -f infra/docker-compose.yml down",
    "infra:logs": "docker compose --project-directory infra -f infra/docker-compose.yml logs -f",
    "profile:dev": "npm run -w services/profile-service start:dev",
    "profile:start": "npm run -w services/profile-service start",
    "address:dev": "npm run -w services/address-service start:dev",
    "address:start": "npm run -w services/address-service start",
    "services:dev": "concurrently -n profile,address \"npm run profile:dev\" \"npm run address:dev\"",
    "services:start": "concurrently -n profile,address \"npm run profile:start\" \"npm run address:start\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
